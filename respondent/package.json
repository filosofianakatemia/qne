{
  "name": "qne-respondent",
  "version": "0.0.1",
  "description": "qne respondent client",
  "scripts": {
    "clean": "rimraf dist && rimraf typings",
    "prebuild": "rimraf dist",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install",
    "postinstall-build": "npm run postinstall && npm run build",
    "api": "json-server --watch mock.api.json --port 3004",
    "dev": "webpack-dev-server --inline --progress --port 8080",
    "start": "concurrently \"npm run api\" \"npm run dev\" ",
    "test": "karma start"
  },
  "dependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/router": "^3.0.0",
    "@ngrx/router-store": "^1.1.0",
    "@ngrx/core": "^1.0.0",
    "@ngrx/store": "^2.0.0",
    "@ngrx/effects": "2.0.0-beta.3",
    "ngrx-store-freeze": "^0.1.0",
    "core-js": "^2.2.2",
    "rxjs": "^5.0.0-beta.12",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "css-loader": "^0.23.1",
    "concurrently": "^2.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "json-server": "^0.8.19",
    "karma": "^0.13.22",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta",
    "webpack-merge": "^0.14.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "contributors": [
    "Timo Tiuraniemi <timo.tiuraniemi@iki.fi>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filosofianakatemia/qne/issues"
  },
  "homepage": "http://qne.io"
}
